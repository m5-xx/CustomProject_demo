<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mayday.xy.customproject">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/bubble_14"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.mayday.tool.localMusicManager.MyMusicMainInterface"
            android:persistableMode="persistAcrossReboots" />

        <service android:name="com.mayday.ShowPlayMusic.PlayService" />

        <activity android:name=".BaseActivity"></activity>

        <activity android:name="com.mayday.tool.localMusicManager.ShowLyc"/>

        <activity android:name="com.mayday.tool.localMusicManager.MyLikeMusicInfo"/>

        <activity android:name="com.mayday.mv.tool.VideoActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="sensorLandscape"
            />


    </application>

</manifest>